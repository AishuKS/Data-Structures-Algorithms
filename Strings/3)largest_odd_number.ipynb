   "source": [
    "LARGEST ODD NUMBER\n",
    "Input: num = \"1250\"\n",
    "Output: \"125\"\n",
    "\n",
    "Input: num = \"2468\"\n",
    "Output: \"\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "d8a71d06-56f6-42c7-a74b-f8d71e195381",
