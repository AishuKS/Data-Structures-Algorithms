   "outputs": [],
   "source": [
    "def largestOddNumberInString(num):\n",
    "    i = len(num) - 1\n",
    "    while i >= 0:\n",
    "        if int(num[i])%2 != 0:\n",
    "            return num[:i+1]\n",
    "        i = i - 1\n",
    "    return \"\""
   ]
