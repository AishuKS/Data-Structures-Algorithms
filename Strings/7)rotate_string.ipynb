   "source": [
    "ROTATE STRING\n",
    "Input: a = 'vwxyz' b = 'xyzvw'\n",
    "Output: True\n",
    "\n",
    "Input: a = 'vwxyz' b = 'xyzwv'\n",
    "Output: False"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "22b11f80-8755-4d9f-b5c3-d78588582ea7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def rotateString(a,b):\n",
    "    for i in range(len(a)):\n",
    "        if b[0] == a[i]:\n",
    "            if b == a[i:] + a[:i]:\n",
    "                return True\n",
    "    return False"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
